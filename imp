function getCompleteQuantitiyObject(id){
	var QuantityClass = Parse.Object.extend("Quantity");
	var query = new Parse.Query(QuantityClass);
	query.get(id, {
	 success: function(quantity) {
	   // The object was retrieved successfully.
	   console.log(quantity);
	 },
	 error: function(object, error) {
	   // The object was not retrieved successfully.
	   // error is a Parse.Error with an error code and message.
	 }
	});
}


function getCompleteSelectDishObject(id){
	var SelectDishClass = Parse.Object.extend("SelectDish");
	var query = new Parse.Query(SelectDishClass);
	query.get(id, {
	 success: function(quantity) {
	   // The object was retrieved successfully.
	   console.log(quantity);
	 },
	 error: function(object, error) {
	   // The object was not retrieved successfully.
	   // error is a Parse.Error with an error code and message.
	 }
	});
}



